echo "[Install] heroku toolbelt"
  brew install heroku-toolbelt

echo "[Install] heroku-accounts plugin"
  heroku plugins:install git://github.com/ddollar/heroku-accounts.git

echo "[Install] heroku-config plugin" # Local environment from ENV
  heroku plugins:install git://github.com/ddollar/heroku-config.git

echo "[User Input] Set up Heroku Accounts"
  open https://github.com/ddollar/heroku-accounts
